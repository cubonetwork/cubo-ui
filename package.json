{
  "name": "cubo-ui-srcs",
  "version": "1.8.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "build": "ng build application --prod --base-href 'https://cubonetwork.github.io/cubo-ui/'",
    "deploy": "npm run build && ngh --repo=https://GH_TOKEN@github.com/cubonetwork/cubo-ui.git --name='Travis CI' --email=dev@cubo.network --message='New release ðŸŽ‰' --no-silent",
    "build:lib": "ng build lib --prod",
    "deploy:lib": "npm run build:lib && npm publish projects/lib/dist",
    "test": "ng test lib --watch=false",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tslint -c tslint.json 'projects/**/*.ts'",
    "lint:css": "sass-lint -c sass-lint.json 'projects/**/*.scss' -v -q",
    "fix:ts": "tslint -c tslint.json 'projects/**/*.ts' --fix",
    "prepush": "npm run test",
    "precommit": "npm run lint",
    "postinstall": "npm run build:lib && npm link ./projects/lib/dist",
    "analyzer": "npm run build && webpack-bundle-analyzer ./dist/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/cdk": "^6.4.5",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/flex-layout": "github:angular/flex-layout-builds",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/material": "^6.4.5",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/router": "^6.1.2",
    "@ngx-meta/core": "^6.0.0",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.12.0",
    "ng-lazyload-image": "^4.0.0",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.3",
    "@angular-devkit/build-ng-packagr": "~0.7.3",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.7.0",
    "codelyzer": "~4.4.4",
    "cubo-schematics": "^1.2.9",
    "husky": "^0.14.3",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.0",
    "ng-packagr": "^4.1.0",
    "protractor": "~5.4.0",
    "rupture-sass": "^0.3.0",
    "sass-lint": "^1.12.1",
    "ts-node": "^7.0.1",
    "tsickle": ">=0.32.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
