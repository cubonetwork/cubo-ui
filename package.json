{
  "name": "cubo-ui-srcs",
  "version": "1.2.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "build": "ng build application --prod --base-href 'https://cubonetwork.github.io/cubo-ui/'",
    "build:lib": "ng build lib --prod",
    "deploy": "npm run build && ngh --repo=https://GH_TOKEN@github.com/cubonetwork/cubo-ui.git --name='Travis CI' --email=dev@cubo.network --message='New release ðŸŽ‰' --no-silent",
    "test": "ng test lib",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tslint -c tslint.json 'projects/**/*.ts'",
    "lint:css": "sass-lint -c sass-lint.json 'projects/**/*.scss' -v -q",
    "fix:ts": "tslint -c tslint.json 'projects/**/*.ts' --fix",
    "prepush": "npm run test",
    "precommit": "npm run lint",
    "analyzer": "npm run build && webpack-bundle-analyzer ./dist/stats.json"
  },
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/flex-layout": "github:angular/flex-layout-builds",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngx-meta/core": "^6.0.0-rc.1",
    "core-js": "^2.5.5",
    "highlight.js": "^9.12.0",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.0.3",
    "codelyzer": "~4.3.0",
    "cubo-schematics": "^1.0.8",
    "husky": "^0.14.3",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "^2.4.2",
    "protractor": "~5.3.1",
    "rupture-sass": "^0.2.3",
    "sass-lint": "^1.12.1",
    "ts-node": "^6.0.2",
    "tsickle": ">=0.27.5",
    "tslib": "^1.9.0",
    "tslint": "~5.10.0",
    "typescript": "^2.7.2",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
