{
  "name": "cubo-ui-srcs",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --port 4300",
    "build": "ng build application --prod --base-href 'https://cubonetwork.github.io/cubo-ui/'",
    "deploy": "npm run build && ngh --repo=https://GH_TOKEN@github.com/cubonetwork/cubo-ui.git --name='Travis CI' --email=dev@cubo.network --message='New release 🎉' --no-silent",
    "build:lib": "ng build lib --prod",
    "deploy:lib": "npm run build:lib && npm publish projects/lib/dist",
    "test": "ng test lib --watch=false",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tslint -c tslint.json 'projects/**/*.ts'",
    "lint:css": "sass-lint -c sass-lint.json 'projects/**/*.scss' -v -q",
    "fix:ts": "tslint -c tslint.json 'projects/**/*.ts' --fix",
    "postinstall": "npm run build:lib && npm link ./projects/lib/dist",
    "analyzer": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build application --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^10.2.1",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.1",
    "@angular/compiler": "^10.2.1",
    "@angular/core": "^10.2.1",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.1.3",
    "@angular/http": "^7.2.16",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^8.0.2",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "^8.0.2",
    "@ngx-meta/core": "^7.0.10",
    "core-js": "^3.1.4",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.14.2",
    "node-sass": "^4.12.0",
    "rxjs": "^6.6.3",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "~10.1.3",
    "@cypress/browserify-preprocessor": "^2.1.1",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "browser-sync": "^2.26.7",
    "codelyzer": "^6.0.0",
    "concurrently": "^4.1.2",
    "cubo-schematics": "^2.0.0",
    "cypress": "^3.8.1",
    "cypress-cucumber-preprocessor": "^1.16.2",
    "cypress-file-upload": "^3.5.1",
    "express": "^4.15.2",
    "husky": "^3.0.2",
    "imagemin-power-cli": "^3.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "rupture-sass": "^0.3.0",
    "sass-lint": "^1.12.1",
    "start-server-and-test": "^1.7.11",
    "ts-loader": "^6.0.4",
    "ts-node": "~8.3.0",
    "tsify": "^4.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "uuid": "^3.3.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.0",
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/language-service": "^8.0.2",
    "ng-packagr": "^5.3.0",
    "tsickle": ">=0.34.3",
    "tslib": "^1.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
