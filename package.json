{
  "name": "cubo-ui-srcs",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --port 4300",
    "build": "ng build application --prod --base-href 'https://cubonetwork.github.io/cubo-ui/'",
    "deploy": "npm run build && ngh --repo=https://GH_TOKEN@github.com/cubonetwork/cubo-ui.git --name='Travis CI' --email=dev@cubo.network --message='New release ðŸŽ‰' --no-silent",
    "build:lib": "ng build lib --prod",
    "deploy:lib": "npm run build:lib && npm publish projects/lib/dist",
    "test": "ng test lib --watch=false",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "tslint -c tslint.json 'projects/**/*.ts'",
    "lint:css": "sass-lint -c sass-lint.json 'projects/**/*.scss' -v -q",
    "fix:ts": "tslint -c tslint.json 'projects/**/*.ts' --fix",
    "prepush": "npm run test",
    "precommit": "npm run lint",
    "postinstall": "npm run build:lib && npm link ./projects/lib/dist",
    "analyzer": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build application --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@ngx-meta/core": "^7.0.0",
    "core-js": "^2.6.1",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.13.1",
    "ng-lazyload-image": "^5.0.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.4",
    "@angular-devkit/build-ng-packagr": "~0.11.4",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "cubo-schematics": "^1.2.9",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.5",
    "protractor": "~5.4.2",
    "rupture-sass": "^0.3.0",
    "sass-lint": "^1.12.1",
    "ts-node": "^7.0.1",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.12.0",
    "typescript": "^3.1.6",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
